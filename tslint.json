{
    "rules": {
        "arrow-return-shorthand":{
            "severity": "warning"
        },
        "ban-comma-operator": true,
        "class-name": true,
        "comment-format": [true, "check-space"],
        "deprecation": {
            "severity": "warning"
        },
        "curly":true,
        "new-parens": true,
        "no-angle-bracket-type-assertion": true,
        "no-arg": true,
        "no-boolean-literal-compare": true,
        "no-conditional-assignment": true,
        "no-console": {
            "severity": "warning",
            "options": [
                "debug",
                "info",
                "log",
                "time",
                "timeEnd",
                "trace"
            ]
        },
        "no-debugger":{
            "severity": "warning"
        },
        "no-duplicate-imports": true,
        "no-duplicate-super": true,
        "no-duplicate-variable": true,
        "no-eval": true,
        "no-for-in-array": {
            "severity": "warning"
        },
        "no-irregular-whitespace": true,
        "no-mergeable-namespace": true,
        "no-misused-new": true,
        
        "no-sparse-arrays": true,
        "no-string-literal": true,
        "no-switch-case-fall-through": true,
        "no-this-assignment": true,
        "no-use-before-declare": true,
        "no-unused-variable":{
            "severity": "warning"
        },
        "no-var-keyword": true,
        "no-var-requires": true,
        "one-variable-per-declaration": [true, "ignore-for-loop"],
        "prefer-for-of": {
            "severity": "warning"
        },
        "prefer-method-signature":{
            "severity": "warning"
        },
        "prefer-switch":{
            "severity": "warning"
        },
        "prefer-template": {
            "severity": "warning"
        },
        "semicolon": true,
        "switch-default": true,
        "trailing-comma": true,
        "typeof-compare": true,
        "unified-signatures": {
            "severity": "warning"
        }
    },
    "jsRules": {
        
    }

}